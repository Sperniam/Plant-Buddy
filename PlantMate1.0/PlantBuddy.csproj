<Project Sdk="Microsoft.NET.Sdk.Maui">


<PropertyGroup>
    <JavaSdkDirectory>C:\Program Files\Java\jdk-17</JavaSdkDirectory>
    <AcceptAndroidSDKLicenses>true</AcceptAndroidSDKLicenses>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>net8.0-android;net8.0-ios;net8.0-maccatalyst;net8.0-windows10.0.19041.0</TargetFrameworks>
    <UseMaui>true</UseMaui>
    <SingleProject>true</SingleProject>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Maui.Controls" Version="8.0.100" />
  </ItemGroup>

</Project>
